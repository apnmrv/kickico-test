{
    "name": "kickico/json-data-client",
    "description": "A Laravel composer package to receive JSON encoded data stored in a predefined format",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Ponomarev",
            "email": "apnmrv@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kickico\\JsonDataClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kickico\\JsonDataClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0@dev",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0",
        "mockery/mockery": "~1.2",
        "orchestra/testbench": "~3.0|~4.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "laravel/framework": "^6.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kickico\\JsonDataClient\\JsonDataClientServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
